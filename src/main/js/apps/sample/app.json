{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "preFetchBundles": false,
        "allowedBundles": [
            "system",
            "console",
            "notifier",
            "splashscreen",
            "templatelayout",
            "template-seasons",
            "theme-everlasting",
            "theme-winter",
            "theme-spring",
            "theme-summer",
            "theme-autumn",
            "map-init",
            "map-preload-2D",
            "toolset",
            "toolrules",
            "banner",
            "basemaptoggler",
            "mapnavigation",
            "popups-default",
            "scalebar",
            "locateme",
            "share-link",
            "printing",
            "toc",
            "dn_printingenhanced",
            "authentication"
        ],
        "require": [
            "${app}.app"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "themes": {
            "ThemeModel": {
                "_selectedTheme": "everlasting"
            },
            "ThemeSelector": {
                "componentEnabled": true
            }
        },
        "dn_printingenhanced": {
            "Config": {
                "showPrintPreview": true,
                "showAdvancedOptions": true,
                "showDpiSelect": true,
                "layoutTemplatesInfoTaskName": "Get Layout Templates Info Task",
                "defaultPageUnit": "CENTIMETER",
                "defaultFormat": "PDF",
                "defaultTemplate": "A4 Portrait",
                "defaultDpi": 96,
                "dpiValues": [
                    {
                        "value": 96,
                        "text": "96"
                    },
                    {
                        "value": 150,
                        "text": "150"
                    },
                    {
                        "value": 300,
                        "text": "300"
                    }
                ],
                "hideFormats": [
                    "GIF",
                    "EPS",
                    "SVG",
                    "SVGZ"
                ],
                "hideTemplates": [
                    "Letter ANSI A Landscape",
                    "Letter ANSI A Portrait",
                    "Tabloid ANSI B Landscape",
                    "Tabloid ANSI B Portrait"
                ],
                "useUsernameAsAuthor": true,
                "usernameAttributes": [
                    "givenname",
                    "sn"
                ],
                "customTextElements": [
                    {
                        "email": "${mail}"
                    },
                    {
                        "city": "${city}"
                    },
                    {
                        "country": "${country}"
                    },
                    {
                        "phonenumber": "${phonenumber}"
                    },
                    {
                        "sn": "${sn}"
                    },
                    {
                        "street": "${street}"
                    },
                    {
                        "testString": "testString"
                    }
                ]
            }
        },
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "image": "resource('${app}:/images/logo_conterra.png')",
                "imageWidth": 200,
                "imagePosition": "above",
                "link": "http://developernetwork.conterra.de"
            }
        },
        "basemaptoggler": {
            "Config": {
                "nextBasemap": "hybrid"
            }
        },
        "map-init": {
            "Config": {
                "basemaps": [
                    {
                        "id": "esri_street",
                        "title": "Straßenkarte (grau)",
                        "thumbnailUrl": "resource('${app}:/images/streets.png')",
                        "basemap": "gray-vector"
                    },
                    {
                        "id": "esri_hybrid",
                        "title": "Luftbild (hybrid)",
                        "thumbnailUrl": "resource('${app}:/images/hybrid.png')",
                        "basemap": "hybrid"
                    },
                    {
                        "id": "esri_topo",
                        "title": "Topographische Karte",
                        "thumbnailUrl": "resource('${app}:/images/topo.png')",
                        "selected": true,
                        "basemap": {
                            "type": "GROUP",
                            "layers": [
                                {
                                    "basemap": "topo-vector",
                                    "type": "AGS_VECTORTILE",
                                    "url": "https://www.arcgis.com/sharing/rest/content/items/6f65bc1351b7411588a8cb43fe23dad7/resources/styles/root.json"
                                },
                                {
                                    "id": "esri_topo1",
                                    "type": "AGS_TILED",
                                    "url": "https://services.arcgisonline.com/arcgis/rest/services/Elevation/World_Hillshade/MapServer"
                                }
                            ]
                        }
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "koeln1",
                            "title": "${map.koeln1.title}",
                            "description": "Basisdaten der Stadt Köln",
                            "url": "https://services.conterra.de/arcgis/rest/services/common/koeln/MapServer",
                            "type": "AGS_DYNAMIC",
                            "visible": false,
                            "minScale": 500000,
                            "maxScale": 250000,
                            "sublayers": [
                                {
                                    "title": "${map.koeln1.districts.title}",
                                    "id": 11,
                                    "opacity": 0.5,
                                    "minScale": 500000,
                                    "maxScale": 250000,
                                    "popupTemplate": {
                                        "title": "{STV_NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.districts.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "STV_NR",
                                                        "label": "${common.number}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln1.boroughs.title}",
                                    "id": 10,
                                    "opacity": 0.5,
                                    "minScale": 500000,
                                    "maxScale": 250000,
                                    "popupTemplate": {
                                        "title": "{NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.boroughs.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "expression/area",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "expression/area-rel",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "NUMMER",
                                                        "label": "${common.number}"
                                                    }
                                                ]
                                            }
                                        ],
                                        "expressionInfos": [
                                            {
                                                "name": "area",
                                                "title": "${common.area}",
                                                "expression": "$feature.FLAECHE / 10000"
                                            },
                                            {
                                                "name": "area-rel",
                                                "title": "${common.totalArea}",
                                                "expression": "Round(($feature.FLAECHE / 405020000)*100,2)"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln1.precints.title}",
                                    "id": 9,
                                    "opacity": 0.5,
                                    "minScale": 500000,
                                    "maxScale": 250000,
                                    "popupTemplate": {
                                        "title": "{NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.precints.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "expression/area",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "expression/area-rel",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "NUMMER",
                                                        "label": "${common.number}"
                                                    }
                                                ]
                                            }
                                        ],
                                        "expressionInfos": [
                                            {
                                                "name": "area",
                                                "title": "${common.area}",
                                                "expression": "$feature.FLAECHE / 10000"
                                            },
                                            {
                                                "name": "area-rel",
                                                "title": "${common.totalArea}",
                                                "expression": "Round(($feature.FLAECHE / 405020000)*100,2)"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ],
                    "ground": {
                        "url": "https://elevation3d.arcgis.com/arcgis/rest/services/WorldElevation3D/Terrain3D/ImageServer",
                        "type": "ELEVATION"
                    }
                },
                "view": {
                    "viewmode": "2D",
                    "extent": {
                        "xmin": 600000,
                        "ymin": 5800000,
                        "xmax": 1700000,
                        "ymax": 7600000,
                        "spatialReference": 3857
                    }
                }
            }
        },
        "templates": {
            "TemplateModel": {
                "_templates": [
                    {
                        "name": "seasons",
                        "widgets": [
                            {
                                "widgetRole": "drawer_button",
                                "attachTo": "map_topleft",
                                "props": {
                                    "layoutIndex": 0
                                }
                            },
                            {
                                "widgetRole": "sharelink",
                                "window": {
                                    "dockable": false,
                                    "dockingBar": null
                                }
                            },
                            {
                                "widgetRole": "legend",
                                "sublayout": [
                                    "mobile_portrait",
                                    "mobile_landscape"
                                ],
                                "window": {
                                    "dockable": false,
                                    "dockingBar": null
                                }
                            },
                            {
                                "widgetRole": "printing",
                                "template": "seasons",
                                "sublayouts": [
                                    "desktop",
                                    "tablet_landscape",
                                    "tablet_portrait"
                                ],
                                "priority": 1,
                                "noMerge": true,
                                "window": {
                                    "dockTool": "printingToggleTool",
                                    "marginBox": {
                                        "l": 20,
                                        "t": 125,
                                        "w": 435,
                                        "h": 550
                                    }
                                }
                            },
                            {
                                "widgetRole": "printing",
                                "template": "seasons",
                                "sublayout": [
                                    "mobile_portrait"
                                ],
                                "attachTo": "full-screen-window",
                                "nomerge": true,
                                "priority": 1,
                                "window": {
                                    "dockable": true,
                                    "dockTool": "printingToggleTool",
                                    "dockingBar": "${id}-dockingBarBottomLeft"
                                }
                            },
                            {
                                "widgetRole": "banner",
                                "sublayout": [
                                    "mobile_portrait"
                                ],
                                "attachTo": "footer_right"
                            }
                        ]
                    }
                ]
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "locateMeTool",
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted"
                    },
                    {
                        "id": "drawer_left",
                        "title": "Werkzeuge",
                        "cssClass": "ct-main-app-menu",
                        "tools": [
                            "printingEnhancedToggleTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "drawer_button"
                        },
                        "container": "ignore",
                        "windowType": "drawer_left"
                    }
                ]
            }
        },
        "toolrules": {
            "ToolActiveStateManager": {
                "activateOnStartToolIds": [
                    "printingEnhancedToggleTool"
                ]
            }
        }
    }
}
