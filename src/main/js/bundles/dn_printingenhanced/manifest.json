{
    "name": "dn_printingenhanced",
    "version": "1.0.0-SNAPSHOT",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "vendor": "con terra GmbH",
    "keywords": [
        "print",
        "AGS"
    ],
    "icon": {},
    "license": {},
    "productName": "devnet-mapapps-printing-enhanced",
    "dependencies": {
        "printing": "^4.0.0",
        "coordinatetransformer": "^4.0.0"
    },
    "CSS-Themes-Extension": [
        {
            "name": "*",
            "files": [
                "./css/styles.css"
            ]
        }
    ],
    "components": [
        {
            "name": "Config",
            "impl": "./PrintingPreviewController",
            "immediate": true,
            "provides": [
                "dn_printingenhanced.PrintingEnhancedController"
            ],
            "properties": {
                "showPrintPreview": true,
                "layoutTemplatesInfoTaskName": "Get Layout Templates Info Task",
                "defaultPageUnit": "CENTIMETERS",
                "defaultFormat": null,
                "defaultTemplate": null,
                "hideFormats": [],
                "hideTemplates": []
            },
            "references": [
                {
                    "name": "_printingWidget",
                    "providing": "dijit.Widget",
                    "filter": "(widgetRole=printing)"
                },
                {
                    "name": "_printingPreviewDrawer",
                    "providing": "dn_printingenhanced.PrintingPreviewDrawer"
                },
                {
                    "name": "tool",
                    "providing": "ct.tools.Tool",
                    "filter": "(id=printingToggleTool)",
                    "cardinality": "0..1"
                },
                {
                    "name": "mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel",
                    "cardinality": "0..1"
                },
                {
                    "name": "_logger",
                    "providing": "ct.framework.api.LogService",
                    "cardinality": "0..1"
                }
            ]
        },
        {
            "name": "PrintingPreviewDrawer",
            "provides": [
                "dn_printingenhanced.PrintingPreviewDrawer"
            ],
            "references": [
                {
                    "name": "_mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                },
                {
                    "name": "_coordinateTransformer",
                    "providing": "ct.api.coordinatetransformer.CoordinateTransformer"
                }
            ]
        }
    ]
}
